.groupbuy_body{
  width: 100%;
  height: 100%;
  background: red;
  box-sizing: border-box;
}
.groupbuy_Succeed{
  width: 100%;
  height: 100%;
  background: #fff;
}
.succeed_header{
  width: 100%;
  background: #fff;
  font-size: 32rpx;
  text-align: center;
  padding-top: 60rpx;
  box-sizing: border-box;
  line-height: 1;
}
.succeed_img{
  width: 151rpx;
  height: 151rpx;
  margin-bottom: 20rpx;
}
.succeed_title{
  color:#333;
  line-height: 1;
}
.succeed_hint{
  width: 100%;
  background: #fff;
  font-size: 24rpx;
  line-height: 1;
  color:#999;
  text-align: center;
  margin-top: 30rpx;
}
.QrCode{
  text-align: center;
  font-size: 24rpx;
  background: #fff;
}
.QrCode_img{
  width: 300rpx;
  height: 300rpx;
  margin: 30rpx auto;
  margin-bottom: 0;
}
.QrCode_title{
  color:#999;
  line-height: 1;
  margin-top: 30rpx;
}
.groupbuy_attention{
  width: 100%;
  background: #fff;
}
.attention_hint{
  text-align: center;
  color:#F2922F;
  font-size: 24rpx;
  line-height: 1;
  margin-top: 20rpx;
}
.takeCode{
  font-size: 24rpx;
  color:#999;
  line-height: 1;
  margin-top: 60rpx;
}
.codeNum{
  color:#F2922F;
}
.takeCode_button{
  width: 200rpx;
  height: 60rpx;
  background: #fff;
  border:1px solid #F2922F;
  text-align: cneter;
  line-height: 60rpx;
  margin:20rpx auto 0 auto;
  color:#F2922F;
  border-radius: 6rpx;
}
.gohome_button{
  width:250rpx;
  height:88rpx;
  background:#f2922f;
  border-radius:8rpx;
  color:#fff;
  line-height:88rpx;
  font-size:32rpx;
  border-color:#f2922f;
  text-align: center;
  margin:0 auto;
  margin-top: 20rpx;
}