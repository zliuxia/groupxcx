.groupbuy_body{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.groupbuy_content{
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  padding-top: 260rpx;
}
.groupbuy_banner{
  width: 100%;
  margin-bottom: 20rpx;
  box-shadow: 0 2px 8px 0 rgba(0,0,0,0.12); 
  border-radius: 6rpx;
  overflow: hidden;
  box-sizing: border-box;
  padding-bottom:30rpx;
  position: relative;
}
.nextRight{
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color:#F2922F;
}
.nextimg{
  width: 14rpx;
  height: 24rpx;
  margin-left: 10rpx;
}
.bannerimg{
  width: 100%;
  height: 100%;
}
.groupbuy_banner:last-child{
  margin-bottom: 0;
}
.lodeing{
  width: 100%;
  height: 80rpx;
  background: #f8f8f8;
  color:#333;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
}
.groupbuyFooter{
  width:100%;
  height: 130rpx;
  background: #fff;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.footerTitle{
  width: 150rpx;
  height: 50rpx;
