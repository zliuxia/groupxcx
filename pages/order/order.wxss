  font-size: 24rpx;
  color:#333;
  display: flex;
  align-items: center;
  line-height: 1;
  margin-bottom: 6rpx;
}
.footerimg{
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.groupbuySlogan{
  font-size: 20rpx;
  color:#999;
  line-height: 1;
}
.nullData{
