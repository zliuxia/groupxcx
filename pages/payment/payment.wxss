page{
  height: 100%;
}
.payment_content{
  height: 100%;
}
.formTop{
  padding-left: 30rpx;
}
.groupbuy_from{
  position: relative;
  height: 100%;
  overflow-y: hidden;
}
.gtoupInput{
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #ebebeb;
  font-size: 28rpx;
  color:#333;
}
.gtoupInput input{
  color:#333;
}
.labeltext{
  width: 200rpx;
  font-size: 28rpx;
  color: #999;
}
.textareasection{
  border-bottom: 1rpx solid #ebebeb;
}
.groupbuyTextarea{
  font-size: 28rpx;
  color:#333;
  height: 192rpx;
  position: static;
  z-index: 0;
  text-indent: 0rpx;
  padding-left: 0rpx;
}
.textarealabel{
  height: 68rpx;
  line-height: 68rpx;
  display: block;
}
.primaryFooter{
  width: 100%;
  height: 100rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom:0;
  z-index: 200;
  box-sizing: border-box;
  box-shadow: 0 -4px 6px 0 rgba(0,0,0,0.06);
}
.primaryButton{
  width: 200rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  background: #F2922F;
  color:#fff;
  font-size: 32rpx;
  border-radius: 0;
  position: absolute;
  right: 0;
  margin:0;
  padding:0;
  border:none;
}
button::after{ border: none; }
.primaryTotal{
  width: 550rpx;
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  box-sizing: border-box;
  padding-left: 30rpx;
  font-size: 28rpx;
  color:#333;
}
.formStation{
  width: 100%;
  border-top:20rpx solid #f8f8f8;
  box-sizing: border-box;
  padding-left: 30rpx;
  margin-top: -1rpx;
}
.pitchOnStation{
  padding:14rpx 0 18rpx 0;
  border-bottom: 1rpx solid #ebebeb;
}
.Stationtitle{
  width: 100%;
  box-sizing: border-box;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color:#666;
  border-bottom: 1rpx solid #ebebeb;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.rightNextimg{
  width: 18rpx;
  height: 31rpx;
  margin-right: 30rpx;
}
.pitchOnTitle,.StationAddress{
  font-size: 28rpx;
  color:#666;
  margin-top: 15rpx;
}
.StationName{
  font-size: 28rpx;
  color:#333;
}
.takeStation_content{
  width: 100%;
  height: 100%;
  padding-top: 88rpx;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  z-index: 300;
  background: #fff;
  overflow-y: scroll;
}
.takeStation_title{
  width: 100%;
  height: 88rpx;
  background: #fff;
  font-size: 28rpx;
  line-height: 88rpx;
  padding-left: 30rpx;
  color:#333;
  border-bottom: 1rpx solid #dbdbdb;
  border-top:1rpx solid #dbdbdb;
  position: absolute;
  z-index: 100;
  top: 0;
  box-sizing: border-box;
}
.takeStation_list{
  width: 100%;
  background: #fff;
  padding-left: 30rpx;
  box-sizing: border-box; 
  overflow-y:scroll;
  position: absolute;
  top: 88rpx;
}
.tackStation{
  width: 100%;
  background: #fff;
  border-bottom: 1rpx solid #dbdbdb;
  padding:26rpx 0 28rpx 0;
  box-sizing: border-box;
  min-height: 140rpx;
  position: relative;
}
.tackname{
  font-size: 32rpx;
  color:#666;
  margin-bottom: 10rpx;
}
.tackAddress{
  font-size: 24rpx;
  color:#999;
}
.tackradio{
  position: absolute;
  right: 30rpx;
  top: 40rpx;
  display: none;
}
.stationActive{
  background: red;
}
.stationActive .tackname{
  color:#F2922F;
}
.stationActive .tackAddress{
  color:#F2922F;
}
.showStation {
  top: 100%;
  height: 100%;
  overflow-y: scroll;
}
.col-c6{
  color: #c6c6c6;
}
.openBindPhone{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  position: fixed;
  z-index: 300;
  top: 0;
  box-sizing: border-box;
}
.bingPhoneCentent{
  width: 524rpx;
  height: 658rpx;
  border-radius: 15rpx;
  position: relative;
  margin:-329rpx auto 0 auto;
  top: 50%;
}
.bingPhonebg{
  width: 524rpx;
  height: 658rpx;
  border-radius: 15rpx;
}
.bindPhoneCenter{
    width: 100%;
    height: 455rpx;
    position: absolute;
    bottom: 0;
}
.bindPhoneMagess{
  width: 100%;
  padding-top: 140rpx;
}
.bindPhoneTitle{
  font-size: 32rpx;
  color:#333;
  font-weight: bold;
  text-align: center;
  line-height: 1;
  margin-bottom: 26rpx;
}
.bindPhoneText{
  font-size: 28rpx;
  color:#999;
  text-align: center;
  line-height: 1;
  margin-bottom: 16rpx;
}
.confirmButton{
  width: 444rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color:#fff;
  background: #F2922F;
  border-radius: 8rpx;
  margin:0 auto;
  position: absolute;
  bottom: 40rpx;
  left: 40rpx;
  right: 40rpx;
}
.closeBindPhone{
  width: 90rpx;
  height: 90rpx;
  position: absolute;
  top: 0;
  right: 0;
  display: block;
}
.closeX{
  width: 30rpx;
  height: 30rpx;
  display: block;
  margin:0 auto;
  position: relative;
  top: 50%;
  margin-top: -15rpx;
}