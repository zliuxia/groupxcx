.selectCommunity{
  width: 100%;
  height: 300rpx;
}
.selectCommunity_header{
  width: 100%;
  height: 394rpx;
  position: relative;
}
.selectBg{
  width: 100%;
  height: 324rpx;
}
.selectCenter{
  width: 100%;
  height: 394rpx;
  position: absolute;
  z-index: 100;
  top: 0;
  left: 0;
}
.Community_title{
  width: 100%;
  height: 88rpx;
  background: #fff;
  line-height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.text_center{
  font-size: 32rpx;
  color:#333;
  text-align: center;
}
.iconTab{
  width: 44rpx;
  height: 2rpx;
}
.text_center{
  box-sizing: border-box;
  padding:0 20rpx;
}
.locationLogo{
  width: 100%;
  text-align: center;
  box-sizing: border-box;
  padding-top: 20rpx;
}
.locationLogoimg{
  width: 104rpx;
  height: 104rpx;
}
.loactionTitle{
  width: 100%;
  font-size: 24rpx;
  color:#fff;
  line-height: 1;
  margin-top: 20rpx;
}
.locationAddress{
  font-size: 30rpx;
  color:#fff;
  width: 100%;
  text-align: center;
  line-height: 1;
  margin-top: 30rpx;
}
.selectCommunityButton{
  font-size: 28rpx;
  color:#333;
  text-align: center;
  width: 380rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  box-sizing: border-box;
  border:4rpx solid #F2922F;
  border-radius: 40rpx;
  margin:0 auto;
  margin-top: 40rpx;
}
.Community_list_body{
  width: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
}
.Community_list{
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  border-bottom: 1rpx solid #ebebeb;
  display: flex;
  align-items: center;
  padding-right: 30rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.communListimg{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
.communListRight{
  height: 100%;
  flex: 1;
}
.addressTitle{
  display: flex;
  width: 100%;
  justify-content: space-between;
  font-size: 28rpx;
  color:#333;
  line-height: 1;
  margin-bottom: 16rpx;
}
.fetchgoodsAddress{
  font-size: 24rpx;
  color:#666;
  line-height: 38rpx;
  width: 570rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.colonelname{
  font-size: 24rpx;
  color:#999;
  line-height: 1;
  margin-bottom: 20rpx;
}
.distance{
  font-size: 24rpx;
  color:#F2922F;
  line-height: 1;
}
.locationCenterActive{
  width: 690rpx;
  height: 270rpx;
  background: #fff;
  border-radius: 8rpx;
  box-shadow: 0 2px 8px 0 rgba(0,0,0,0.12);
}
.selectCenterActive .locationLogo{
  position: absolute;
  z-index: 300;
}
.selectCenterActive .locationLogoimg{
  width: 130rpx;
  height: 130rpx;
}
.selectCenterActive .loactionTitle{
  color:#999;
}
.locationCenterActive{
  position: absolute;
  top: 72rpx;
  left: 30rpx;
  box-sizing: border-box;
  padding-top: 122rpx;
}
.communityName{
  width: 100%;
  font-size: 30rpx;
  color:#333;
  text-align: center;
  line-height: 1;
  margin-top: 14rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: bold;
}
.distanceAddressName{
  font-size: 24rpx;
  color:#666;
  box-sizing: border-box;
  padding:0 64rpx;
  margin-top: 20rpx;
  line-height: 38rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.selectCenterActive .loactionTitle{
  margin-top: 0;
}
.historyCommunity_list_body{
  border-bottom:20rpx solid #f8f8f8; 
}
.historyCommunity_list_body .Community_list{
  border:none;
}
.nullData{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}
.nullcenter{
  width: 500rpx;
}
.nullimg{
  width: 200rpx;
  height: 200rpx;
  margin:0 auto;
  display: block;
}
.nullTitle{
  font-size: 24rpx;
  color:#999;
  text-align: center;
  margin-top:20rpx;
}
.loginPopout{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  position: fixed;
  top: 0;
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}
.PopoutCenter{
  width: 590rpx;
  height: 360rpx;
  background: #fff;
  text-align: center;
  padding:30rpx;
  box-sizing: border-box;
  padding-top: 50rpx;
}
.PopoutCenter .footerTitle{
  width: 200rpx;
  margin-bottom: 30rpx;
}
.PopoutCenter .footerText{
  font-size: 32rpx;
}
.PopoutCenter .groupbuySlogan{
  font-size: 32rpx;
}
.getUserInfoButton{
  margin-top: 50rpx;
}
.footerTitle{
  width: 150rpx;
  height: 50rpx;
  margin:0 auto;
  font-size: 24rpx;
  color:#333;
  display: flex;
  align-items: center;
  line-height: 1;
  margin-bottom: 6rpx;
}
.footerimg{
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.groupbuySlogan{
  font-size: 20rpx;
  color:#999;
  line-height: 1;
}
.communListLeft {
  padding-top: 10rpx;
}
.sitename {
  width:410rpx;
  line-height: 36rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: -5rpx;
}
.hissitename {
  width: 580rpx;
  line-height: 36rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: -5rpx;
}