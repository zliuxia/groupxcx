.groupbuy_body{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding-bottom: 100rpx;
}
.groupbuy_content{
  width: 100%;
  background: #fff;
  padding:30rpx;
  box-sizing: border-box;
}
.index_location_button{
  width: 100%;
  height: 88rpx;
  background: #fff;
  font-size: 28rpx;
  color:#333;
  line-height: 88rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
  display: flex;
  align-items: center;
}
.index_location_view {
  width:100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.index_location_view text {
  float: left;
  
}
.icon_next{
  width: 30rpx;
  height: 18rpx;
  margin-left: 20rpx;
}
.groupbuy_banner_tab{
  width: 690rpx;
  height: 280rpx;
  margin: 0 auto;
  background: #f0f0f0;
  border-radius: 8rpx;
}
button{
  margin-bottom: 30rpx;
}
button:last-child{
  margin-bottom: 0;
}
.page-section-title{
  padding: 0;
}
.swiper-item{
  display: block;
  height: 150px;
  border-radius: 8rpx;
}
.page-section-title{
  margin-top: 60rpx;
  position: relative;
}
.info{
  position: absolute;
  right: 0;
  color: #353535;
  font-size: 30rpx;
}
.page-foot{
  margin-top: 50rpx;
}
.groupbuy_Class_tab{
  width: 100%;
  height: 88rpx;
  background: #fff;
  line-height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1rpx solid #ebebeb;
}
.text_center{
  font-size: 32rpx;
  color:#333;
  text-align: center;
}
.icon_tab{
  width: 54rpx;
  height: 32rpx;
}
.text_center{
  box-sizing: border-box;
  padding:0 20rpx;
}
.goodsList{
  width: 100%;
  height: 280rpx;
  background: #fff;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0 30rpx;
  position: relative;
  border-bottom: 1rpx solid #ebebeb;
}
.list_leftimg{
  width: 220rpx;
  height: 220rpx;
  box-sizing: border-box;
  margin-right: 20rpx;
}
.goodsImg{
  width: 220rpx;
  height: 220rpx;
}
.goods_name{
  font-size: 28rpx;
  color:#333;
  line-height: 38rpx;
  margin-bottom: 5rpx;
}
.goods_sellNum{
  font-size: 24rpx;
  color:#F2922F;
  border:2rpx solid #F2922F;
  display: inline-block;
  padding:0 16rpx;
  height: 38rpx;
  line-height: 38rpx;
  border-radius: 20rpx;
  margin-bottom: 14rpx;
}
.over_time {
  margin-bottom: 14rpx;
}
.over_time,.contrast_price{
  font-size: 24rpx;
  color:#999;
  line-height: 1;
}
.contrast_price {
  text-decoration: line-through;
}
.sale_price{
  font-size: 36rpx;
  color:#EF0000;
  line-height: 1;
  margin-right: 10rpx;
}
.goodscart{
  width: 76rpx;
  height: 76rpx;
  position: absolute;
  bottom:0rpx;
  right: 0rpx;
}
.goodscartimg{
  width: 76rpx;
  height: 76rpx;
}
.shade_body{
  width: 220rpx;
  height: 220rpx;
  background: rgba(f,f,f,0.25);
  position: absolute;
  top: 30rpx;
  left: 30rpx;
}
.shade_state{
  width: 140rpx;
  height: 140rpx;
  background: rgba(0,0,0,0.52);
  color:#fff;
  border-radius: 50%;
  line-height: 140rpx;
  text-align: center;
  font-size: 28rpx;
  position: absolute;
  top: 40rpx;
  left: 40rpx;
}
swiper{
  height: 280rpx;
}
.bannerSwiperimg{
  width: 100%;
  height: 280rpx;
  border-radius: 8rpx;
}
.getfromidbutton{
  width: 100%;
  height: 100%;
  background: transparent;
  color:transparent;
  box-sizing: border-box;
  border:none;
  position: absolute;
  left: 0;
  z-index: 1;
}
button::after{ border: none; }
.lodeing{
  width: 100%;
  height: 80rpx;
  background: #f8f8f8;
  color:#333;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
}
.lodeing{
  width: 100%;
  height: 80rpx;
  background: #f8f8f8;
  color:#333;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
}
.groupbuyFooter{
  width:100%;
  height: 130rpx;
  background: #fff;
  padding-top: 30rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0rpx;
  z-index: 300;
}
.footerTitle{
  width: 150rpx;
  height: 50rpx;
  margin:0 auto;
  font-size: 24rpx;
  color:#333;
  display: flex;
  align-items: center;
  line-height: 1;
  margin-bottom: 6rpx;
}
.footerimg{
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.groupbuySlogan{
  font-size: 20rpx;
  color:#999;
  line-height: 1;
}
.nullData{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}
.nullcenter{
  width: 500rpx;
}
.nullimg{
  width: 200rpx;
  height: 200rpx;
  margin:0 auto;
  display: block;
}
.nullTitle{
  font-size: 24rpx;
  color:#999;
  text-align: center;
  margin-top:20rpx;
}
.price {
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
}
.list_rightCenter {
  position: relative;
  height: 220rpx;
  width: 450rpx;
}