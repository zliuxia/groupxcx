.takeStation_content{
  padding-top: 110rpx;
  box-sizing: border-box;
  background: #F8F8F8;
}
.takeStation_title{
  width: 100%;
  height: 88rpx;
  background: #fff;
  font-size: 28rpx;
  line-height: 88rpx;
  padding-left: 30rpx;
  color:#333;
  border-bottom: 1rpx solid #dbdbdb;
  border-top:1rpx solid #dbdbdb;
  position: fixed;
  z-index: 100;
  top: 0;
}
.takeStation_input{
  width: 100%;
  height: 110rpx;
  background: #fff;
  font-size: 28rpx;
  line-height: 110rpx;
  padding:0 30rpx;
  color:#333;
  border-top:1px solid #dbdbdb;
  border-bottom: 1px solid #dbdbdb;
  position: fixed;
  z-index: 100;
  top:0;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.takeStation_list{
  width: 100%;
  box-sizing: border-box; 
}
.tackStation{
  width: 100%;
  background: #fff;
  border-bottom: 1rpx solid #dbdbdb;
  padding:26rpx 0 28rpx 0rpx;
  box-sizing: border-box;
  min-height: 140rpx;
  padding-left: 30rpx;
}
.tackname{
  font-size: 32rpx;
  color:#666;
  margin-bottom: 10rpx;
}
.tackAddress{
  font-size: 24rpx;
  color:#999;
}
.tackradio{
  position: absolute;
  right: 30rpx;
  top: 40rpx;
  display: none;
}
.stationActive {
  margin-bottom: 20rpx;
  display: block;
}
.stationActive text:nth-child(2){
  font-size: 24rpx;
  color: #F2922F;
  border: 1rpx solid #F2922F;
  padding: 0 10rpx;
  margin-left: 10rpx;
  border-radius: 6rpx;
}
.showStation {
  top: 100%;
  height: 100%;
  overflow-y: scroll;
}
.search_button{
  width: 100rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #5ADAD0;
  text-align: center;
  border-radius: 6rpx;
  margin-left: 30rpx;
  box-sizing: border-box;
}
.search_input{
  width: 100%;
  height: 68rpx;
  line-height: 68rpx;
  background: #eee;
  border:1px solid #eee;
  font-size: 24rpx;
  border-radius: 47rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  padding:0 30rpx 0 24rpx;
}
.input_view{
  height: 68rpx;
  line-height: 68rpx;
  box-sizing: border-box;
  padding-left: 10rpx;
  flex: 1;
}
.search_img{
  width: 30rpx;
  height: 30rpx;
}
.nullData{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}
.nullcenter{
  width: 500rpx;
  height: 600rpx;
}
.nullimg{
  width: 200rpx;
  height: 200rpx;
  margin:0 auto;
  display: block;
}
.nullTitle{
  font-size: 24rpx;
  color:#999;
  text-align: center;
  margin-top:20rpx;
}